<%= flash[:notice] %>

<h4><span class="page-title">ジャンル編集</span></h4>

<!-- ジャンル編集機能ここから -->
<div class="position-center">
　<%= form_with model: ([:admin, @genre]), local: true do |f| %>
　　<%= f.label :ジャンル名 %>
　　<%= f.text_field :name, autofocus: true, placeholder: "ジャンル名" %>
　　<%= f.submit "変更を保存", class:"btn btn-success"  %>
　<% end %>
</div>
<!-- ジャンル編集機能ここまで -->